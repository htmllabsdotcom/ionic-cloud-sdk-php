{
    "name": "wcatron/ionic-cloud-sdk-php",
    "description": "The unofficial library for Ionic Cloud for PHP.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Weston Catron",
            "email": "wcatron@catrondevelopment.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "firebase/php-jwt": "^4.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6"
    },
    "autoload": {
        "psr-4": {
            "Ionic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ionic\\Test\\": "tests/src"
        }
    }
}
